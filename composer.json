{
    "name": "morieti/discount-generator",
    "description": "This package helps you to create and use various types of discount codes in your Laravel project",
    "keywords": [
        "laravel",
        "discount",
        "generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Pouretemadi",
            "email": "pouretemadi.morteza@gmail.com",
            "homepage": "https://morteza-pouretemadi.com"
        }
    ],
    "require": {
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Morieti\\DiscountGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morieti\\Tests\\DiscountGenerator\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Morieti\\DiscountGenerator\\DiscountGeneratorServiceProvider"
            ],
            "aliases": {
                "Discount": "Morieti\\DiscountGenerator\\Facades\\Discount"
            }
        }
    }
}
